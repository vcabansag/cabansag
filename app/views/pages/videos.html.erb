<% @videos.each do |video| %>

  <h2><%= video.title %></h2>
  <h5><%= video.date %></h5>
  <div class="flex-video">
    <iframe src='//player.vimeo.com/video/<%="#{video.url}" %>' width="500" height="375" frameborder="0" webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe>
  </div>

<% end %>
